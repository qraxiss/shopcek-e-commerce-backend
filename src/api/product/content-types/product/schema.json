{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::variant.variant",
      "mappedBy": "product"
    },
    "description": {
      "type": "text"
    },
    "sizes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::size.size",
      "inversedBy": "products"
    },
    "colors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::color.color",
      "inversedBy": "products"
    },
    "image": {
      "type": "string"
    },
    "price": {
      "type": "float"
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::item.item",
      "mappedBy": "product"
    },
    "printful_id": {
      "type": "biginteger"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "wishlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "items"
    }
  }
}
